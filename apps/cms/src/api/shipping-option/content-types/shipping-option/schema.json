{
  "kind": "collectionType",
  "collectionName": "shipping_options",
  "info": {
    "singularName": "shipping-option",
    "pluralName": "shipping-options",
    "displayName": "Shipping Option",
    "description": "Shipping options"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "currency": {
      "type": "string",
      "default": "SAR"
    },
    "estimatedDays": {
      "type": "integer"
    },
    "region": {
      "type": "enumeration",
      "enum": ["saudi", "international"],
      "required": true
    },
    "active": {
      "type": "boolean",
      "default": true
    }
  }
}

